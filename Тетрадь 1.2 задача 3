#include <iostream>
#include <vector>
using namespace std;
int main(){
	int left = 0,x=0;
	vector<int> A = { -5,-3,-2,-1,2,4 };
	int right = A.size() - 1;
	int a = 0, y = A.size() - 1;
	int mid,res=-1;
	while (left <= right) {
		mid = (left + right) / 2;
		if (A[mid] == x) {
			res = mid;
			if (mid < A.size() / 2) {
				cout << "otrisatelnih chisel bolshe";
			}
			else {
				cout << "polozhitelnih chisel bolshe";
			}
			break;
		}
		if (A[mid]<x) {
			left = mid + 1;
		}
		if (A[mid] > x) {
			right = mid - 1;
		}
	}
	if (res == -1) {
		if (x > A[A.size() - 1]) {
			cout << "otrisatelnih chisel bolshe";
		}
		else if (x < A[0]) {
			cout << "polozhitelnih chisel bolshe";
		}
		else {
			while (a != -1) {
				a++;
				if (x > A[a] and x<A[a+1]) {
					break;
				}
			}
			if (a + 1 > A.size() / 2) {
				cout << "otrisatelnih chisel bolshe";
			}
			else {
				cout << "polozhitelnih chisel bolshe";
			}
		}	
	}	
}
